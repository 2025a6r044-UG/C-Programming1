//Program to see if alphabet is vowel or not
#include<stdio.h>
#include<ctype.h>
int main(){
  char ch;
  printf("Enter character:");
  scanf("%c",&ch);
  if(isalpha(ch)){
    ch=toupper(ch);
    if(ch=='A'||ch=='E'||ch=='I'||ch=='O'||ch=='U')
    printf("Vowel");
    else
    printf("Not vowel");
 }
  else 
    printf("invalid");
  return 0;
}

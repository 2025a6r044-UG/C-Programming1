#include<stdio.h>
#include<math.h>
int main(){
    int sum=0,num,temp,dig,con=0;
    scanf("%d",&num);
    temp=num;
    while(temp!=0){
        temp=temp/10;
        con++;
    }
    temp=num;
    while(temp!=0){
        dig=temp%10;
        sum=sum+pow(dig,con);
        temp=temp/10;
    }
    if(sum==num){
        printf("Armstrong");
    }
    else
        printf("Not Armstrong");
    return 0;
}

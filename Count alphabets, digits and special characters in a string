#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    char str[100];
    int counta=0,countd=0,counts=0;
    gets(str);
    for(int i=0;i<strlen(str);i++){
        if(isalpha(str[i])){
            counta++;
        }
        else if(isdigit(str[i])){
            countd++;
        }
        else{
            counts++;
        }
    }
    printf("%d %d %d",counta,countd,counts);
    return 0;
}

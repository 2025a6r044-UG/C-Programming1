#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>
#include <ctype.h>

int main() {
    char str[1000];
    int count=0,i=0;
    scanf("%[^\n]%*c",str);
    while(str[i]!='\0'){
        if(isalpha(str[i])){
            str[i] = tolower(str[i]);
        
        if(str[i]=='a'||str[i]=='e'||str[i]=='i'||str[i]=='o'||str[i]=='u'){
        count++;
        }
        }
        i++;
    }
        printf("%d",count);
        
    return 0;
}

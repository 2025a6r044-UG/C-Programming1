#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    char str[100];
    int countu=0,countl=0;
    gets(str);
    for(int i=0;i<strlen(str);i++){
        if(isupper(str[i])){
            countu++;
        }
        else if(islower(str[i])){
            countl++;
        }
    }
    printf("%d %d",countu,countl);
    return 0;
}

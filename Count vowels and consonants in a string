#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    char str[100];
    int count1=0,count2=0;
    gets(str);
    for(int i=0;i<strlen(str);i++){
        if(isalpha(str[i])){
            str[i]=tolower(str[i]);
            if(str[i]=='a'||str[i]=='e'||str[i]=='i'||str[i]=='o'||str[i]=='u'){
                count1++;
            }
            else
                count2++;
        }
    }
    printf("%d %d",count1,count2);
    return 0;
}

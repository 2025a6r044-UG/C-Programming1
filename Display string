//WAP to take input and display string
#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    char name[30],branch[30],sec[4],coll[30];
    int roll;
    scanf("%d\n",&roll);
    scanf("%[^\n]%*c",name);
    scanf("%[^\n]%*c",branch);
    scanf("%[^\n]%*c",sec);
    scanf("%[^\n]%*c",coll);
    printf("Roll Number: %d\n",roll);
    printf("Name: %s\n",name);
    printf("Branch: %s\n",branch);
    printf("Section: %s\n",sec);
    printf("College: %s",coll);
    
    return 0;
}

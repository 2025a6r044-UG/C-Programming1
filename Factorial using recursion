//WAP to find factorial of a number usinf recursion
#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>
int factorial(int num){
    if(num==0)
        return 1;
    else{
        return num*factorial(num-1);
    }
}
int main() {
    int n,i;
    scanf("%d",&n);
    i=factorial(n);
    printf("%d",i);
    return 0;
}

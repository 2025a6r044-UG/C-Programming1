//WAP to print largest or maximum number among N
#include<stdio.h>
int main(){
    int i,n,num,max=0;
    printf("Enter value of N:");
    scanf("%d",&n);
    for(i=1;i<=n;i++){
        printf("Enter number:");
        scanf("%d",&num);
        if(num>max)
        max=num;
    }
 printf("Max=%d",max);   
 return 0;   
}

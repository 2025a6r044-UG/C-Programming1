#include<stdio.h>
int main(){
    printf("size of char is %d bytes\n",sizeof(char));
    printf("Size of int is %d bytes",sizeof(int));
    printf("Size of float %d bytes",sizeof(float));
    printf("Size of double is %d bytes",sizeof(double));
    printf("Size of long double is %d bytes",sizeof(long double));
    return 0;
}

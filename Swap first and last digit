#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int N,last,first,count;
    scanf("%d",&N);
    if(N>=0 && N<=9)
        printf("%d",N);
    else{
        int temp=N;
        while(temp!=0){
            temp=temp/10;
            count++;
        }
        first=N/pow(10,count-1);
        N=N%(int)pow(10,count-1);
        last=N%10;
        N=N/10;
        N=last*pow(10,count-1)+N*10+first;
        printf("%d",N);
    }
    return 0;
}

#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>
struct student{
    char name[50];
    int age;
    char grade;
};
int main() {
    int n;
    scanf("%d ",&n);
    struct student s1[n];
    for(int i=0;i<n;i++){
        scanf("%[^\n]%*c ",s1[i].name);
        scanf("%d ",&s1[i].age);
        scanf("%c ",&s1[i].grade);
    }
    for(int i=0;i<n;i++){
        printf("Student %d:\n",i+1);
        printf("Name: %s\n",s1[i].name);
        printf("Age: %d\nGrade: %c\n",s1[i].age,s1[i].grade);
    }
    
    return 0;
}

